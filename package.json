    "react-dev-utils": "^3.0.2",
    "react-dom": "^16.0.0",
    "react-error-overlay": "^1.0.9",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.0",
