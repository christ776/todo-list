    "file-loader": "0.11.2",
    "firebase": "^4.6.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest-cli": "^21.2.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
