{
    "env": {
        "browser": true,
        "commonjs":true,
        "es6":true,
        "node": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "extends":"airbnb",
    "rules":{
      "arrow-body-style": [2, "as-needed"],
       // code arrangement matter
       "no-use-before-define": [ 2, { "variables": false } ]
    }
}
